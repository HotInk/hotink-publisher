<div id="edit_front_page" class="clearfix">
	<p class="back_link"><%= link_to '&laquo; Back to control panel', account_control_panel_path(@account) %></p>
	<% form_for([@account, @front_page]) do |f| %>
	  <%= f.error_messages %>
		<%= render :partial => "form", :locals => { :f => f } %>
	<% end %>
</div>
<% content_for :javascript do -%>
	<script type="text/javascript" charset="utf-8">
		load_button_fix(); //fix firefox button display
	</script>
<% end -%>